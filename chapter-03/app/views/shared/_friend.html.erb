<div id="header-story" class="component-auto">
  <% @friends.each do |user| %>
    <div class="mult-language story-micro" id="mult-language" data-user-id=<%= user.id%> title=<%=user.name%>>
      <%= gravatar_for(user, size: 80) %>
      <%= image_tag("https://images6.fanpop.com/image/photos/41300000/Nissh-ki-The-Rising-Sun-Japan-Flag-Today-japan-41319152-375-250.jpg", size: "40x40", :class => "img-add-friend")%>
    </div>
  <% end %>
</div>
<div class="next_right component-auto" data-next-page="<%= @next_page%>" data-total-pages="<%=@total_pages%>"><%= image_tag("data:image/jpeg;base64,", size: "40x80")%></div>

<script id="story-micro-template" type="text/x-handlebars-template">
  {{#each users}}
    <div class="mult-language story-micro" id="mult-language" data-user-id="{{id}}" title="{{name}}">
      <img alt="" class="gravatar" src={{avatar}}>
      <%= image_tag("https://images6.fanpop.com/image/photos/41300000/Nissh-ki-The-Rising-Sun-Japan-Flag-Today-japan-41319152-375-250.jpg", size: "40x40", :class => "img-add-friend")%>
    </div>
  {{/each}}
</script>
